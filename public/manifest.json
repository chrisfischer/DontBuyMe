{
  "short_name": "DBM",
  "name": "Don't Buy Me!",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Don't Buy Me!"
  },
  "content_scripts": [{
    "matches": ["*://*.amazon.com/*"],
    "js": ["edit_cart.js"]
  }],
  "permissions": [
	  "https://*/",
	  "tabs"
  ],
  "version": "1.0"
}
